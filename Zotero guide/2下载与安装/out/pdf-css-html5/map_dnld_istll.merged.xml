<troubleshooting class="- topic/topic troubleshooting/troubleshooting " id="unique_6" topicrefclass="- map/topicref " nd:nd-id="ts_download" oid="ts_download" specializations="@props/deliveryTarget">
    <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">下载失败</ph></title>
    <troublebody class="- topic/body troubleshooting/troublebody ">
        <condition class="- topic/section troubleshooting/condition ">
            <title class="- topic/title ">Condition</title>
            <p class="- topic/p ">Zotero安装包下载失败。</p>
        </condition>
        <troubleSolution class="- topic/bodydiv troubleshooting/troubleSolution ">
            <cause class="- topic/section troubleshooting/cause ">
                <title class="- topic/title ">Cause</title>
                <p class="- topic/p ">网络中断或不稳定。<image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/mac下载/ts%20网络中断.jpg" id="unique_6_Connect_42_image_k3k_f1z_ldc" nd:nd-id="image_k3k_f1z_ldc" placement="inline" width="300"/></p>
            </cause>
            <remedy class="- topic/section troubleshooting/remedy ">
                <title class="- topic/title ">Remedy</title>
                <responsibleParty class="- topic/p troubleshooting/responsibleParty ">IT Center</responsibleParty>
                <steps class="- topic/ol task/steps ">
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">切换WiFi，或使用有线网络、手机热点。</cmd>
                    </step>
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd " xml:lang="zh-CN">重新尝试下载。</cmd>
                    </step>
                </steps>
            </remedy>
        </troubleSolution>
        <troubleSolution class="- topic/bodydiv troubleshooting/troubleSolution ">
            <cause class="- topic/section troubleshooting/cause ">
                <title class="- topic/title ">Cause</title>
                <p class="- topic/p ">电脑内存不足</p>
            </cause>
            <remedy class="- topic/section troubleshooting/remedy " id="unique_6_Connect_42_remedy_vds_bbz_ldc" nd:nd-id="remedy_vds_bbz_ldc">
                <title class="- topic/title ">Remedy</title>
                <responsibleParty class="- topic/p troubleshooting/responsibleParty ">Personal Computer</responsibleParty>
                <steps class="- topic/ol task/steps ">
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">清理电脑内存，确保可用空间大于安装包大小。</cmd>
                    </step>
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">重新尝试下载。</cmd>
                    </step>
                </steps>
            </remedy>
            <remedy class="- topic/section troubleshooting/remedy ">
                <title class="- topic/title ">更多问题请访问</title>
                <steps-informal class="- topic/section task/steps-informal " id="unique_6_Connect_42_steps-informal_ld3_sb1_mdc" nd:nd-id="steps-informal_ld3_sb1_mdc"><xref class="- topic/xref " format="html" href="https://forums.zotero.org/discussions" scope="external"/></steps-informal>
            </remedy>
        </troubleSolution>
    </troublebody>
</troubleshooting></task><task class="- topic/topic task/task " id="unique_7" topicrefclass="- map/topicref " nd:nd-id="win_install" oid="win_install" specializations="@props/deliveryTarget">
    <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">Zotero安装（win版）</ph></title>
    <shortdesc class="- topic/shortdesc ">本节介绍如何在Windows系统电脑上安装Zotero。</shortdesc>
    <taskbody class="- topic/body task/taskbody ">
        <prereq class="- topic/section task/prereq " id="unique_7_Connect_42_prereq_bhp_w5z_ldc" nd:nd-id="prereq_bhp_w5z_ldc">确保Zotero安装包已下载完成。</prereq>
        <context class="- topic/section task/context ">
            <p class="- topic/p "/>
        </context>
        <steps class="- topic/ol task/steps ">
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">找到安装包的位置，双击触控板或鼠标左键打开。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/win安装/win安装包.jpg" id="unique_7_Connect_42_image_qfz_wxz_ldc" nd:nd-id="image_qfz_wxz_ldc" placement="inline" width="400"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">点击<uicontrol class="+ topic/ph ui-d/uicontrol ">Next&gt;</uicontrol>。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/win安装/安装1.jpg" id="unique_7_Connect_42_image_f1d_zxz_ldc" nd:nd-id="image_f1d_zxz_ldc" placement="inline" width="400"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">若采用默认安装路径，则选择<uicontrol class="+ topic/ph ui-d/uicontrol ">Standard</uicontrol>后点击<uicontrol class="+ topic/ph ui-d/uicontrol ">Next&gt;</uicontrol>，并跳过第4步；若需自定义安装路径，选择<uicontrol class="+ topic/ph ui-d/uicontrol ">Custom</uicontrol>后点击<uicontrol class="+ topic/ph ui-d/uicontrol ">Next&gt;</uicontrol>。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/win安装/安装2.jpg" id="unique_7_Connect_42_image_fxt_byz_ldc" nd:nd-id="image_fxt_byz_ldc" placement="inline" width="400"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">点击<uicontrol class="+ topic/ph ui-d/uicontrol ">Browse</uicontrol>，选择好安装路径后，点击<uicontrol class="+ topic/ph ui-d/uicontrol ">Next&gt;</uicontrol>。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/win安装/安装3.jpg" id="unique_7_Connect_42_image_xg3_gzz_ldc" nd:nd-id="image_xg3_gzz_ldc" placement="inline" width="400"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">若需要在桌面创建Zotero图标，则勾选<uicontrol class="+ topic/ph ui-d/uicontrol ">On my
                        Desktop</uicontrol>；若需要Zotero图标出现在开始界面菜单，则勾选<uicontrol class="+ topic/ph ui-d/uicontrol ">In my Start Menu
                        Programs folder</uicontrol>。完成后点击<uicontrol class="+ topic/ph ui-d/uicontrol ">Next&gt;</uicontrol>。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/win安装/安装4.jpg" id="unique_7_Connect_42_image_mgw_jzz_ldc" nd:nd-id="image_mgw_jzz_ldc" placement="inline" width="400"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">点击<uicontrol class="+ topic/ph ui-d/uicontrol ">Install</uicontrol>，开始安装。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/win安装/安装5.jpg" id="unique_7_Connect_42_image_zlj_vzz_ldc" nd:nd-id="image_zlj_vzz_ldc" placement="inline" width="400"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">安装完成后，点击Finish</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/win安装/安装6.jpg" id="unique_7_Connect_42_image_rvh_xzz_ldc" nd:nd-id="image_rvh_xzz_ldc" placement="inline" width="400"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">若能在桌面或安装路径下找到Zotero图标，即安装成功。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/win安装/图标.jpg" id="unique_7_Connect_42_image_m2v_111_mdc" nd:nd-id="image_m2v_111_mdc" placement="inline" width="300"/></info>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/win安装/安装路径中的图标.jpg" id="unique_7_Connect_42_image_lyv_311_mdc" nd:nd-id="image_lyv_311_mdc" placement="inline" width="500"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">双击图标，即可启动Zotero。</cmd>
            </step>
        </steps>
    </taskbody>
    <related-links class="- topic/related-links ">
        <link class="- topic/link " format="dita" href="#unique_8" type="task"><?ditaot gentext?><linktext class="- topic/linktext ">Zotero安装（mac版）</linktext><?ditaot genshortdesc?><desc class="- topic/desc ">本节介绍如何在Mac电脑上安装Zotero。</desc></link>
    </related-links>
<troubleshooting class="- topic/topic troubleshooting/troubleshooting " id="unique_9" topicrefclass="- map/topicref " nd:nd-id="ts_win_instal" oid="ts_win_instal" specializations="@props/deliveryTarget">
    <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">安装失败（win版）</ph></title>
    <troublebody class="- topic/body troubleshooting/troublebody ">
        <condition class="- topic/section troubleshooting/condition ">
            <title class="- topic/title ">Condition</title>
            <p class="- topic/p ">在Windows系统电脑上安装Zotero失败。</p>
        </condition>
        <troubleSolution class="- topic/bodydiv troubleshooting/troubleSolution ">
            <cause class="- topic/section troubleshooting/cause ">
                <title class="- topic/title ">Cause</title>
                <p class="- topic/p ">电脑内存不足。</p>
            </cause>
            <remedy class="- topic/section troubleshooting/remedy ">
                <title class="- topic/title ">Remedy</title>
                <responsibleParty class="- topic/p troubleshooting/responsibleParty ">Personal Computer</responsibleParty>
                <steps class="- topic/ol task/steps ">
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">清理电脑内存，确保可用空间大于软件大小。</cmd>
                    </step>
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">重新尝试安装。</cmd>
                    </step>
                </steps>
            </remedy>
        </troubleSolution>
        <troubleSolution class="- topic/bodydiv troubleshooting/troubleSolution ">
            <cause class="- topic/section troubleshooting/cause " id="unique_9_Connect_42_cause_omd_n31_mdc" nd:nd-id="cause_omd_n31_mdc">
                <title class="- topic/title ">Cause</title>
                <p class="- topic/p ">安装包版本与电脑型号不匹配。</p>
            </cause>
            <remedy class="- topic/section troubleshooting/remedy " id="unique_9_Connect_42_remedy_pmd_n31_mdc" nd:nd-id="remedy_pmd_n31_mdc">
                <title class="- topic/title ">Remedy</title>
                <responsibleParty class="- topic/p troubleshooting/responsibleParty ">Personal Computer</responsibleParty>
                <steps class="- topic/ol task/steps " id="unique_9_Connect_42_steps_qmd_n31_mdc" nd:nd-id="steps_qmd_n31_mdc">
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">找到<uicontrol class="+ topic/ph ui-d/uicontrol ">此电脑</uicontrol>，点击<uicontrol class="+ topic/ph ui-d/uicontrol ">属性</uicontrol>，查看电脑位数</cmd>
                        <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/win安装/ts%20此电脑属性.jpg" id="unique_9_Connect_42_image_kg3_qm1_mdc" nd:nd-id="image_kg3_qm1_mdc" placement="inline" width="300"/></info>
                    </step>
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">在<uicontrol class="+ topic/ph ui-d/uicontrol ">系统类型</uicontrol>中查看电脑位数，检查与安装包版本是否一致。</cmd>
                        <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/win安装/ts%20看位数.jpg" id="unique_9_Connect_42_image_vw1_xm1_mdc" nd:nd-id="image_vw1_xm1_mdc" placement="inline" width="500"/></info>
                    </step>
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">如果不一致，则重新下载相应的安装包进行安装，详见"<xref class="- topic/xref " href="#unique_5" type="task"><?ditaot usertext?>Zotero下载（其他版本）<?ditaot genshortdesc?><desc class="- topic/desc ">本节介绍如何在下载其他版本或系统的Zotero安装包。</desc></xref>"一节。</cmd>
                    </step>
                </steps>
            </remedy>
            <remedy class="- topic/section troubleshooting/remedy ">
                <title class="- topic/title ">更多问题请访问</title>
                <steps-informal class="- topic/section task/steps-informal " id="unique_9_Connect_42_steps-informal_ld3_sb1_mdc" nd:nd-id="steps-informal_ld3_sb1_mdc"><xref class="- topic/xref " format="html" href="https://forums.zotero.org/discussions" scope="external"/></steps-informal>
            </remedy>
        </troubleSolution>
    </troublebody>
</troubleshooting><troubleshooting class="- topic/topic troubleshooting/troubleshooting " id="unique_10" topicrefclass="- map/topicref " nd:nd-id="ts_win_cannot_find" oid="ts_win_cannot_find" specializations="@props/deliveryTarget">
    <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">无法找到Zotero图标（win版）</ph></title>
    <troublebody class="- topic/body troubleshooting/troublebody ">
        <condition class="- topic/section troubleshooting/condition ">
            <title class="- topic/title ">Condition</title>
            <p class="- topic/p ">找不到Zotero的图标，无法使用。</p>
        </condition>
        <troubleSolution class="- topic/bodydiv troubleshooting/troubleSolution ">
            <cause class="- topic/section troubleshooting/cause ">
                <title class="- topic/title ">Cause</title>
                <p class="- topic/p ">安装时未勾选<uicontrol class="+ topic/ph ui-d/uicontrol ">On my Desktop</uicontrol>。</p>
            </cause>
            <remedy class="- topic/section troubleshooting/remedy ">
                <title class="- topic/title ">Remedy</title>
                <steps class="- topic/ol task/steps ">
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">若安装时选择<uicontrol class="+ topic/ph ui-d/uicontrol ">Standard</uicontrol>，则打开电脑C盘，打开<uicontrol class="+ topic/ph ui-d/uicontrol ">Program
                                Files
                            (x86)</uicontrol>文文件夹，进入<uicontrol class="+ topic/ph ui-d/uicontrol ">Zotero</uicontrol>文件夹，找到Zotero图标，双击即可使用。</cmd>
                        <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/win安装/安装路径中的图标.jpg" id="unique_10_Connect_42_image_s5g_jk1_mdc" nd:nd-id="image_s5g_jk1_mdc" placement="inline" width="300"/></info>
                    </step>
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">若安装时选择<uicontrol class="+ topic/ph ui-d/uicontrol ">Custom</uicontrol>，则打开安装时选择的路径。例如，图中自定义安装为"D:\Zotero\"，则打开电脑D盘<uicontrol class="+ topic/ph ui-d/uicontrol ">Zotero</uicontrol>文件夹，找到Zotero图标，双击即可使用。</cmd>
                        <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/win安装/ts%20找路径.jpg" id="unique_10_Connect_42_image_kvp_5k1_mdc" nd:nd-id="image_kvp_5k1_mdc" placement="inline" width="300"/></info>
                    </step>
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">若需将Zotero图标添加至桌面，则选中Zotero图标后按右键，点击<uicontrol class="+ topic/ph ui-d/uicontrol ">发送到</uicontrol>&gt;<uicontrol class="+ topic/ph ui-d/uicontrol ">桌面快捷方式</uicontrol>，随后便可在桌面直接打开Zotero。</cmd>
                        <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/win安装/ts%20加logo.jpg" id="unique_10_Connect_42_image_w2n_2m1_mdc" nd:nd-id="image_w2n_2m1_mdc" placement="inline" width="300"/></info>
                    </step>
                </steps>
            </remedy>
        </troubleSolution>
        <troubleSolution class="- topic/bodydiv troubleshooting/troubleSolution ">
            <remedy class="- topic/section troubleshooting/remedy ">
                <title class="- topic/title ">更多问题请访问</title>
                <steps-informal class="- topic/section task/steps-informal " id="unique_10_Connect_42_steps-informal_ld3_sb1_mdc" nd:nd-id="steps-informal_ld3_sb1_mdc"><xref class="- topic/xref " format="html" href="https://forums.zotero.org/discussions" scope="external"/></steps-informal>
            </remedy>
        </troubleSolution>
    </troublebody>
</troubleshooting></task></topic><topic class="- topic/topic " id="unique_11" topicrefclass="- map/topicref " nd:nd-id="install" oid="install" specializations="@props/deliveryTarget">
