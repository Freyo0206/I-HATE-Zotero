    <topicref is-chapter="true" class="- map/topicref " href="#unique_1" type="topic" xml:lang="zh-CN"><topicmeta class="- map/topicmeta " data-topic-id="cover_intro"><resourceid appid="cover_intro" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_1" class="- topic/navtitle ">Zotero简介</navtitle><?ditaot gentext?><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">本章节为您介绍Zotero的基本信息，包括Zotero是什么、Zotero的基本功能、Zotero的优势，帮助您初步认识Zotero。</shortdesc></topicmeta><topicref class="- map/topicref " href="#unique_2" type="concept" xml:lang="zh-CN"><topicmeta class="- map/topicmeta " data-topic-id="what_is_zotero"><resourceid appid="what_is_zotero" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_2" class="- topic/navtitle ">Zotero是什么</navtitle><?ditaot gentext?><?ditaot genshortdesc?><shortdesc class="- map/shortdesc "/></topicmeta></topicref><topicref class="- map/topicref " href="#unique_3" type="concept" xml:lang="zh-CN"><topicmeta class="- map/topicmeta " data-topic-id="functions_of_zotero"><resourceid appid="functions_of_zotero" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_3" class="- topic/navtitle ">Zotero有什么用</navtitle><?ditaot gentext?></topicmeta></topicref><topicref class="- map/topicref " href="#unique_9" type="concept" xml:lang="zh-CN"><topicmeta class="- map/topicmeta " data-topic-id="why_zotero"><resourceid appid="why_zotero" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_9" class="- topic/navtitle ">为什么选择Zotero</navtitle><?ditaot gentext?></topicmeta></topicref></topicref>
    <topicref is-chapter="true" class="- map/topicref " href="#unique_10" type="topic" xml:lang="zh-CN"><topicmeta class="- map/topicmeta " data-topic-id="topic_1_dnld_istll"><resourceid appid="topic_1_dnld_istll" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_10" class="- topic/navtitle ">下载与安装</navtitle><?ditaot gentext?><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">本章节为您介绍如何下载与安装Zotero，以Windows和MacOS两个系统为主，读者可根据实际情况下载和安装适合的版本。</shortdesc></topicmeta><topicref class="- map/topicref " href="#unique_11" type="topic"><topicmeta class="- map/topicmeta " data-topic-id="download_zotero"><resourceid appid="download_zotero" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_11" class="- topic/navtitle ">下载与安装（win版）</navtitle><?ditaot gentext?></topicmeta><topicref class="- map/topicref " href="#unique_12" type="task"><topicmeta class="- map/topicmeta " data-topic-id="download"><resourceid appid="download" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_12" class="- topic/navtitle ">Zotero下载（win版）</navtitle><?ditaot gentext?><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">本节介绍如何在Windows系统电脑上下载Zotero安装包。</shortdesc></topicmeta><topicref class="- map/topicref " href="#unique_15" type="troubleshooting"><topicmeta class="- map/topicmeta " data-topic-id="ts_download"><resourceid appid="ts_download" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_15" class="- topic/navtitle ">下载失败</navtitle><?ditaot gentext?></topicmeta></topicref></topicref><topicref class="- map/topicref " href="#unique_16" type="task"><topicmeta class="- map/topicmeta " data-topic-id="win_install"><resourceid appid="win_install" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_16" class="- topic/navtitle ">Zotero安装（win版）</navtitle><?ditaot gentext?><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">本节介绍如何在Windows系统电脑上安装Zotero。</shortdesc></topicmeta><topicref class="- map/topicref " href="#unique_18" type="troubleshooting"><topicmeta class="- map/topicmeta " data-topic-id="ts_win_instal"><resourceid appid="ts_win_instal" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_18" class="- topic/navtitle ">安装失败（win版）</navtitle><?ditaot gentext?></topicmeta></topicref><topicref class="- map/topicref " href="#unique_19" type="troubleshooting"><topicmeta class="- map/topicmeta " data-topic-id="ts_win_cannot_find"><resourceid appid="ts_win_cannot_find" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_19" class="- topic/navtitle ">无法找到Zotero图标（win版）</navtitle><?ditaot gentext?></topicmeta></topicref></topicref></topicref><topicref class="- map/topicref " href="#unique_20" type="topic"><topicmeta class="- map/topicmeta " data-topic-id="install"><resourceid appid="install" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_20" class="- topic/navtitle ">下载与安装（mac版）</navtitle><?ditaot gentext?></topicmeta><topicref class="- map/topicref " href="#unique_13" type="task"><topicmeta class="- map/topicmeta " data-topic-id="download"><resourceid appid="download" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_13" class="- topic/navtitle ">Zotero下载（mac版）</navtitle><?ditaot gentext?><?ditaot genshortdesc?><shortdesc class="- map/shortdesc ">本节介绍如何在mac电脑上下载Zotero安装包。</shortdesc></topicmeta><topicref class="- map/topicref " href="#unique_21" type="topic"><topicmeta class="- map/topicmeta " data-topic-id="download_ts_mac_0"><resourceid appid="download_ts_mac_0" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_21" class="- topic/navtitle ">下载失败</navtitle><?ditaot gentext?></topicmeta></topicref></topicref><topicref class="- map/topicref " href="#unique_17" type="task"><topicmeta class="- map/topicmeta " data-topic-id="mac_install"><resourceid appid="mac_install" class="- topic/resourceid " oxy-source="topic"/><navtitle href="#unique_17" cla