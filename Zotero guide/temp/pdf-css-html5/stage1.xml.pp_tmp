<concept class="- topic/topic concept/concept " id="unique_89" topicrefclass="- map/topicref " nd:nd-id="Introduction" oid="Introduction" specializations="@props/deliveryTarget">
    <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">Zotero注册登录介绍</ph></title>
    <shortdesc class="- topic/shortdesc ">注册并登录Zotero帐号后，用户可通过Zotero同步和备份文献管理数据，Zotero便捷的云存储和协作功能能够帮助用户更高效地进行文献阅读和写作。</shortdesc>
</concept><task class="- topic/topic task/task " id="unique_90" topicrefclass="- map/topicref " nd:nd-id="zotero" oid="zotero" specializations="@props/deliveryTarget">
    <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">如何注册Zotero帐号</ph></title>
    <shortdesc class="- topic/shortdesc ">介绍如何注册Zotero帐号。</shortdesc>
    <taskbody class="- topic/body task/taskbody ">
        <context class="- topic/section task/context "/>
        <steps class="- topic/ol task/steps ">
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">打开浏览器，输入网址<xref class="- topic/xref " format="html" href="https://www.zotero.org/" scope="external"/>。</cmd>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">在官网页面上，点击页面右上方的“Log In”。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/Homepage-Login.png" id="unique_90_Connect_42_image_x34_cpt_ldc" nd:nd-id="image_x34_cpt_ldc" placement="inline" width="600"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">进入Log in页面后，点击页面中部的“Register for a free account”。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/Login%20to%20Register.jpg" id="unique_90_Connect_42_image_pbf_2pt_ldc" nd:nd-id="image_pbf_2pt_ldc" placement="inline" width="600"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">or：打开Zotero客户端，点击页面上方的“编辑”。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/点击设置.jpg" id="unique_90_Connect_42_image_oww_fpt_ldc" nd:nd-id="image_oww_fpt_ldc" placement="inline" width="600"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">点击“编辑”中的“设置”。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/从设置登录.png" id="unique_90_Connect_42_image_i1c_hpt_ldc" nd:nd-id="image_i1c_hpt_ldc" placement="inline" width="600"/></info>
                <stepresult class="- topic/itemgroup task/stepresult ">出现以下窗口<image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/输入用户名和密码.jpg" id="unique_90_Connect_42_image_ktg_jpt_ldc" nd:nd-id="image_ktg_jpt_ldc" placement="inline" width="600"/></stepresult>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">点击“创建账户”</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/创建账户.jpg" id="unique_90_Connect_42_image_vcz_jpt_ldc" nd:nd-id="image_vcz_jpt_ldc" placement="inline" width="600"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">进入注册页面后，填写用户名、电子邮件地址和密码，随后进行人机验证。</cmd>
                <info class="- topic/itemgroup task/info ">请注意：用户名会生成个性域名，请谨慎设置。</info>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/Register.jpg" id="unique_90_Connect_42_image_djt_kpt_ldc" nd:nd-id="image_djt_kpt_ldc" placement="inline" width="600"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">进行人机身份验证后，点击页面上的“Register”按钮，提交注册信息。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/Confirm.jpg" id="unique_90_Connect_42_image_tyk_lpt_ldc" nd:nd-id="image_tyk_lpt_ldc" placement="inline" width="600"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">前往注册时提供的电子邮件地址，查找来自Zotero的验证邮件，进入邮件中提供的链接。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/E-mail-Confrim.jpg" id="unique_90_Connect_42_image_jnt_mpt_ldc" nd:nd-id="image_jnt_mpt_ldc" placement="inline" width="600"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">进入链接后显示如下页面，则注册成功。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/Successful%20Log%20in.jpg" id="unique_90_Connect_42_image_vqq_npt_ldc" nd:nd-id="image_vqq_npt_ldc" placement="inline" width="600"/></info>
            </step>
        </steps>
    </taskbody>
</task><task class="- topic/topic task/task " id="unique_91" topicrefclass="- map/topicref " nd:nd-id="sign_in" oid="sign_in" specializations="@props/deliveryTarget">
    <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">如何登入Zotero帐号</ph></title>
    <shortdesc class="- topic/shortdesc ">介绍如何登入已完成注册的Zotero帐号。</shortdesc>
    <taskbody class="- topic/body task/taskbody ">
        <prereq class="- topic/section task/prereq " id="unique_91_Connect_42_prereq_exb_5hs_ldc" nd:nd-id="prereq_exb_5hs_ldc">确保已下载Zotero客户端至桌面。</prereq>
        <context class="- topic/section task/context "/>
        <steps class="- topic/ol task/steps ">
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">打开Zotero客户端，点击右侧菜单栏顶端的“同步”。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/Login-1.jpg" id="unique_91_Connect_42_image_msd_jqt_ldc" nd:nd-id="image_msd_jqt_ldc" placement="inline" width="600"/></info>
                <stepresult class="- topic/itemgroup task/stepresult ">点击后出现如下界面<image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/Login-1-result.jpg" id="unique_91_Connect_42_image_ohx_kqt_ldc" nd:nd-id="image_ohx_kqt_ldc" placement="inline" width="600"/></stepresult>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">点击“打开同步选项”。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/打开同步选项.jpg" id="unique_91_Connect_42_image_zss_lqt_ldc" nd:nd-id="image_zss_lqt_ldc" placement="inline" width="600"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">or：打开Zotero客户端，点击页面上方的“编辑”。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/点击设置.jpg" id="unique_91_Connect_42_image_a24_nqt_ldc" nd:nd-id="image_a24_nqt_ldc" placement="inline" width="600"/></info>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">点击“编辑”中的“设置”。</cmd>
                <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/从设置登录.png" id="unique_91_Connect_42_image_tmr_4qt_ldc" nd:nd-id="image_tmr_4qt_ldc" placement="inline" width="600"/></info>
                <stepresult class="- topic/itemgroup task/stepresult ">出现以下窗口<image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/输入用户名和密码.jpg" id="unique_91_Connect_42_image_qmt_pqt_ldc" nd:nd-id="image_qmt_pqt_ldc" placement="inline" width="600"/></stepresult>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">在窗口中输入注册时设置的用户名和密码。</cmd>
            </step>
            <step class="- topic/li task/step ">
                <cmd class="- topic/ph task/cmd ">登录成功。</cmd>
                <stepresult class="- topic/itemgroup task/stepresult "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/登录成功.jpg" id="unique_91_Connect_42_image_nsh_sqt_ldc" nd:nd-id="image_nsh_sqt_ldc" placement="inline" width="600"/></stepresult>
            </step>
        </steps>
    </taskbody>
<troubleshooting class="- topic/topic troubleshooting/troubleshooting " id="unique_92" topicrefclass="- map/topicref " nd:nd-id="forget_password" oid="forget_password" specializations="@props/deliveryTarget">
    <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">忘记密码后如何找回密码</ph></title>
    <troublebody class="- topic/body troubleshooting/troublebody ">
        <condition class="- topic/section troubleshooting/condition ">
            <title class="- topic/title ">Condition</title>
            <p class="- topic/p ">忘记用户名或密码。</p>
        </condition>
        <troubleSolution class="- topic/bodydiv troubleshooting/troubleSolution ">
            <cause class="- topic/section troubleshooting/cause ">
                <title class="- topic/title ">Cause</title>
                <p class="- topic/p ">忘记用户名或密码。</p>
            </cause>
            <remedy class="- topic/section troubleshooting/remedy ">
                <title class="- topic/title ">Remedy</title>
                <steps class="- topic/ol task/steps ">
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">打开Zotero客户端，点击页面上方的“编辑”。</cmd>
                        <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/7文件同步与备份/images/注册和登录/点击设置.jpg" id="unique_92_Connect_42_image_a24_nqt_ldc" nd:nd-id="image_a24_nqt_ldc" placement="inline" width="600"/></info>
