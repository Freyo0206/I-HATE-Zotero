<troubleshooting class="- topic/topic troubleshooting/troubleshooting " id="unique_22" topicrefclass="- map/topicref " nd:nd-id="ts_install_mac" oid="ts_install_mac" specializations="@props/deliveryTarget">
    <title class="- topic/title "><ph class="- topic/ph topic/title-wrapper ">安装失败（mac版）</ph></title>
    <troublebody class="- topic/body troubleshooting/troublebody ">
        <condition class="- topic/section troubleshooting/condition ">
            <title class="- topic/title ">Condition</title>
            <p class="- topic/p ">在Mac电脑上安装Zotero失败。</p>
        </condition>
        <troubleSolution class="- topic/bodydiv troubleshooting/troubleSolution ">
            <cause class="- topic/section troubleshooting/cause ">
                <title class="- topic/title ">Cause</title>
                <p class="- topic/p ">电脑内存不足。</p>
            </cause>
            <remedy class="- topic/section troubleshooting/remedy " id="unique_22_Connect_42_remedy_hfm_md1_mdc" nd:nd-id="remedy_hfm_md1_mdc">
                <title class="- topic/title ">Remedy</title>
                <responsibleParty class="- topic/p troubleshooting/responsibleParty ">Personal Computer</responsibleParty>
                <steps class="- topic/ol task/steps " id="unique_22_Connect_42_steps_vfm_md1_mdc" nd:nd-id="steps_vfm_md1_mdc">
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">清理电脑内存，确保可用空间大于软件大小。</cmd>
                    </step>
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">重新尝试安装。</cmd>
                    </step>
                </steps>
            </remedy>
        </troubleSolution>
        <troubleSolution class="- topic/bodydiv troubleshooting/troubleSolution ">
            <cause class="- topic/section troubleshooting/cause " id="unique_22_Connect_42_cause_al3_btp_ydc" nd:nd-id="cause_al3_btp_ydc">
                <title class="- topic/title ">Cause</title>
                <p class="- topic/p ">Mac电脑系统权限未打开。</p>
            </cause>
            <remedy class="- topic/section troubleshooting/remedy " id="unique_22_Connect_42_remedy_bl3_btp_ydc" nd:nd-id="remedy_bl3_btp_ydc">
                <title class="- topic/title ">Remedy</title>
                <responsibleParty class="- topic/p troubleshooting/responsibleParty ">Personal Computer</responsibleParty>
                <steps class="- topic/ol task/steps " id="unique_22_Connect_42_steps_cl3_btp_ydc" nd:nd-id="steps_cl3_btp_ydc">
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">打开<uicontrol class="+ topic/ph ui-d/uicontrol ">设置</uicontrol></cmd>
                        <info class="- topic/itemgroup task/info "><image class="- topic/image " height="60" href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/mac安装/ts_设置图标.jpg" id="unique_22_Connect_42_image_fj4_1vp_ydc" nd:nd-id="image_fj4_1vp_ydc" placement="inline"/></info>
                    </step>
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">找到<uicontrol class="+ topic/ph ui-d/uicontrol ">隐私与安全性</uicontrol></cmd>
                        <info class="- topic/itemgroup task/info "><image class="- topic/image " height="220" href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/mac安装/ts_隐私与安全性.jpg" id="unique_22_Connect_42_image_kvx_cvp_ydc" nd:nd-id="image_kvx_cvp_ydc" placement="inline"/></info>
                    </step>
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">找到<uicontrol class="+ topic/ph ui-d/uicontrol ">安全性</uicontrol>，选中<uicontrol class="+ topic/ph ui-d/uicontrol ">App
                            Store与已知开发者</uicontrol></cmd>
                        <info class="- topic/itemgroup task/info "><image class="- topic/image " height="110" href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/mac安装/ts_开发者.jpg" id="unique_22_Connect_42_image_d5w_gvp_ydc" nd:nd-id="image_d5w_gvp_ydc" placement="inline"/></info>
                    </step>
                </steps>
            </remedy>
        </troubleSolution>
        <troubleSolution class="- topic/bodydiv troubleshooting/troubleSolution ">
            <cause class="- topic/section troubleshooting/cause " id="unique_22_Connect_42_cause_en5_nd1_mdc" nd:nd-id="cause_en5_nd1_mdc">
                <title class="- topic/title ">Cause</title>
                <p class="- topic/p ">拖动过程有误。</p>
            </cause>
            <remedy class="- topic/section troubleshooting/remedy " id="unique_22_Connect_42_remedy_fn5_nd1_mdc" nd:nd-id="remedy_fn5_nd1_mdc">
                <title class="- topic/title ">Remedy</title>
                <responsibleParty class="- topic/p troubleshooting/responsibleParty ">Personal Computer</responsibleParty>
                <steps class="- topic/ol task/steps " id="unique_22_Connect_42_steps_gn5_nd1_mdc" nd:nd-id="steps_gn5_nd1_mdc">
                    <step class="- topic/li task/step ">
                        <cmd class="- topic/ph task/cmd ">重新拖动图标，直至出现绿色加号，确保将安装包拖入文件夹内</cmd>
                        <info class="- topic/itemgroup task/info "><image class="- topic/image " href="file:/Users/haihai/Desktop/mac研一上/英汉技术写作/I-HATE-Zotero/Zotero%20guide/2下载与安装/图片/mac安装/ts.jpg" id="unique_22_Connect_42_image_vzn_nf1_mdc" nd:nd-id="image_vzn_nf1_mdc" placement="inline" width="300"/></info>
                    </step>
                </steps>
            </remedy>
            <remedy class="- topic/section troubleshooting/remedy ">
                <title class="- topic/title ">更多问题请访问</title>
                <steps-informal class="- topic/section task/steps-informal " id="unique_22_Connect_42_steps-informal_ld3_sb1_mdc" nd:nd-id="steps-informal_ld3_sb1_mdc"><xref class="- topic/xref " format="html" href="https://forums.zotero.org/discussions" scope="external"/></steps-informal>
            </remedy>
        </troubleSolution>
    </troublebody>
</troubleshooting></task></topic><task class="- topic/topic task/task " id="unique_14" topicrefclass="- map/topicref " nd:nd-id="download" oid="download" specializations="@props/deliveryTarget">
